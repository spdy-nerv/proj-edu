.act-footer-wrap {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 86rpx;
  border-top: 1rpx solid #e5e5e5;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  padding: 25rpx 0;
  box-sizing: border-box;
}

.act-list {
  flex: 1;
  height: 36rpx;
  line-height: 36rpx;
  border-right: 1rpx solid #e5e5e5;
  font-size: 32rpx;
  color: #666;
  text-align: center;
}

.act-list-selected {
  color: #00b7ee;
}

.act-list text {
  width: 32rpx;
  height: 34rpx;
  display: inline-block;
  background-image: url(http://edu-1253427581.coscd.myqcloud.com/%E6%B4%BB%E5%8A%A8%E5%88%97%E8%A1%A8.png);
  background-size: 100%;
  background-repeat: no-repeat;
  margin-right: 16rpx;
  vertical-align: middle;
}

.act-list-selected text {
  background-image: url(http://edu-1253427581.coscd.myqcloud.com/%E6%B4%BB%E5%8A%A8%E5%88%97%E8%A1%A8-%E9%80%89%E4%B8%AD.png);
}

