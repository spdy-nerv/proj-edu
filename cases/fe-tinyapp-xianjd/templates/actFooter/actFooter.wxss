  flex: 1;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 32rpx;
  color: #666;
  text-align: center;
}

.personal-center-selected {
  color: #00b7ee;
}

.personal-center text {
  width: 32rpx;
