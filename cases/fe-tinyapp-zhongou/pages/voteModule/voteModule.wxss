.reportTitle{
    padding: 30rpx;
    color: #B29754;
    font-size: 32rpx;
}
.panel-btn-area {
    width: 100%;
    height: 160rpx;
    background-color: #fff;
}

.panel-main {
    height: 100%;
    width: 100%;
    box-sizing: border-box;
}

.panel-main image{
    width: 100%;
    height: 414rpx;
}

.panel-description {
    margin-top: 40rpx;
    font-size: 30rpx;
    color: #B29754;
    line-height: 1.2;
    margin-left: 40rpx;
}

.panel-btn {
    width: 700rpx;
    height: 70rpx;
    margin: auto;
    /* background-image: url(http://eventsocial-1253427581.cossh.myqcloud.com/v1.0/vote_btn.png); */
    background-repeat: no-repeat;
    background-size: 100%;
    background-color:#B29754;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    font-size: 30rpx;
}

.panel-disabled {
    margin-top: 40rpx;
    font-size: 36rpx;
    color: #cccccc;
    text-align: center;
}

.panel-option-wrap {
    padding: 40rpx;
}

.panel-option {
    display: flex;
    flex-direction: row;
}

.panel-option-checkbox {
    width: 32rpx;
    height: 32rpx;
    background-image: url(http://eventsocial-1253427581.cossh.myqcloud.com/v1.0/checkbox.png);
    background-size: 100%;
    margin-right: 15rpx;
}

.option-checked {
    background-image: url(http://eventsocial-1253427581.cossh.myqcloud.com/v1.0/checkbox_selected.png);
}

.panel-option-text {
    font-size: 30rpx;
    color: #2d2e33;
    line-height: 1.2;
    flex: 1;
}

.panel-data {
    margin-top: 20rpx;
    font-size: 28rpx;
    color: #a2a1a1;
}

.panel-data-count {
    margin-right: 40rpx;
}

.panel-progress {
    margin-top: 5rpx;
    height: 30rpx;
    border: 1rpx solid #cccccc;
    border-radius: 8rpx;
    overflow: hidden;
}

.panel-progress-inner {
    height: 100%;
}